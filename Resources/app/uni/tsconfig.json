{
  "compilerOptions": {
    "composite": true,
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": ".",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@static/*": [
        "./src/static/*"
      ]
    },
    "resolveJsonModule": true,
    "types": [
      "@dcloudio/types",
      "@uni-helper/uni-app-types",
      "nutui-uniapp/global.d.ts"
    ],
    "compilerOptions": {
      "allowJs": true,
      "checkJs": false,
      "types": [
        "@uni-helper/vite-plugin-uni-pages"
      ]
    },
    "allowJs": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "vueCompilerOptions": {
    "plugins": [
      "@uni-helper/uni-types/volar-plugin"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ]
}
